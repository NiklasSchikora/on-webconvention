{
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "test -d .ddev/ssl/certs && npm run dev:https || npm run dev:http",
    "dev:https": "nuxt dev --https --ssl-cert .ddev/ssl/certs/master.crt --ssl-key .ddev/ssl/certs/master.key",
    "dev:http": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview"
  },
  "devDependencies": {
    "nuxt": "3.0.0-rc.1",
    "prisma": "^3.14.0"
    "@prisma/client": "^3.14.0",
  }
}
